{
  "name": "@rstest/rsbuild-config-bridge",
  "version": "0.1.0",
  "description": "Bridge utilities for converting Rsbuild config to Rstest config",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rslib build",
    "dev": "rslib build --watch",
    "test": "rstest",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "rstest",
    "rsbuild",
    "bridge",
    "configuration"
  ],
  "license": "MIT",
  "peerDependencies": {
    "@rsbuild/core": "*",
    "@rstest/core": "*"
  },
  "devDependencies": {
    "@rsbuild/core": "^2.0.0-beta.4",
    "@rslib/core": "^0.19.0",
    "@rstest/core": "workspace:*",
    "@rstest/tsconfig": "workspace:*"
  },
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/web-infra-dev/rstest/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/web-infra-dev/rstest",
    "directory": "packages/rsbuild-config-bridge"
  }
}
