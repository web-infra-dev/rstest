{
  "name": "@rstest/reporter-agent-md",
  "version": "0.1.0",
  "description": "LLM-friendly Markdown reporter for Rstest.",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./package.json": {
      "default": "./package.json"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rslib build",
    "dev": "cross-env SOURCEMAP=true rslib build --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@jridgewell/trace-mapping": "0.3.31",
    "@vercel/detect-agent": "^1.0.0",
    "package-manager-detector": "^1.6.0",
    "pathe": "^2.0.3",
    "stacktrace-parser": "0.1.11",
    "strip-ansi": "^7.1.2"
  },
  "peerDependencies": {
    "@rstest/core": "workspace:~"
  },
  "devDependencies": {
    "@rslib/core": "^0.19.0",
    "@rstest/core": "workspace:*",
    "@rstest/tsconfig": "workspace:*",
    "@types/node": "^22.16.5",
    "typescript": "^5.9.3"
  },
  "keywords": [
    "rstest",
    "reporter",
    "markdown",
    "llm"
  ],
  "bugs": {
    "url": "https://github.com/web-infra-dev/rstest/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/web-infra-dev/rstest",
    "directory": "packages/reporter-agent-md"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "engines": {
    "node": ">=18.12.0"
  }
}
